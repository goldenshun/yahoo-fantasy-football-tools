@model IEnumerable<YahooFantasySportsClient.Domain.Team>

@{
    ViewBag.Title = "ListTeams";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>ListTeams</h2>
<ul id="teams">
 @foreach (YahooFantasySportsClient.Domain.Team team in Model)
 {
    <div class="team" style="float:left;">
        <li>Id: @team.Id</li>
        <li>Key: @team.Key</li>
        <li>Name: @team.Name</li>
        <li><ul>
            @foreach (YahooFantasySportsClient.Domain.Player player in team.GetRoster().GetPlayers())
            {
                <li>@player.Name</li>
            }
        </ul></li>
    </div>
 }
 </ul>