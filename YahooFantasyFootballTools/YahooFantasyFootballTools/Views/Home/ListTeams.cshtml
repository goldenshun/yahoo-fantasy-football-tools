@model IEnumerable<Fantasizer.Domain.Team>

@{
    ViewBag.Title = "Teams";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>ListTeams</h2>
<ul id="teams">
 @foreach (Fantasizer.Domain.Team team in Model)
 {
    <li>Id: @team.Id</li>
    <li>Key: @team.Key</li>
     <li>Name: @Html.ActionLink(team.Name, "ListEligibleKeepers", new {teamKey = team.Key}) </li>
     <li>@Html.ActionLink("Analyze Depth", "ShowRosterDepth", new {teamKey = team.Key})</li>
    <li>------------------------</li>
 }
 </ul>