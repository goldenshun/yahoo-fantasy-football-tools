@model IEnumerable<Fantasizer.Domain.Team>

@{
    ViewBag.Title = "Teams";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>ListTeams</h2>
<ul id="teams">
 @foreach (Fantasizer.Domain.Team team in Model)
 {
    <li>Id: @team.Id</li>
    <li>Key: @team.Key</li>
     <li>Name: @Html.ActionLink(team.Name, "ListEligibleKeepers", "Team", new {teamKey = team.Key}, null) </li>
     <li>@Html.ActionLink("Analyze Depth", "ShowRosterDepth", "Team", new {teamKey = team.Key}, null)</li>
    <li>------------------------</li>
 }
 </ul>